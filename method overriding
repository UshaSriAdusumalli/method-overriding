class Language{
  public void displayInfo(){
    System.out.println("common english languagr");
  }
}
class Java extends Language{
  public void displayInfo(){
    System.out.println("Java programming language");
  }
}
class Main{
  public static void main(String[] args){
    Java j1=new Java();
    j1.displayInfo();
    Language l1=new Language();
    l1.displayInfo();
  }
}
